    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.com.bethehero"
    },
    "android": {
      "package": "br.com.bethehero"
    },
    "description": "Projeto be the hero criado em React Native.",
    "githubUrl": "https://github.com/magaum/be-the-hero-mobile"
  }
}
